{
  	"name": "n8n-nodes-expertosenia",
  	"version": "1.0.0",
  "description": "n8n to connect with Expertos en IA API (WhatsApp HTTP API)",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://expertosenia.com",
  "author": {
    "name": "Expertos en IA",
    "email": "soporte@expertosenia.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expertosenia/n8n-nodes-expertos-en-ia.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "test": "jest",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExpertosIAApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ExpertosIA/ExpertosIA.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "ts-jest": "^29.2.5",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
